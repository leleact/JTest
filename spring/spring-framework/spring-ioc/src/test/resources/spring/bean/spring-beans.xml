<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="com.github.leleact.jtest.spring.ioc.bean"/>

    <bean id="commonbeans" class="com.github.leleact.jtest.spring.ioc.bean.CommonBean">
        <property name="id" value="AB"/>
        <property name="num" value="12"/>
    </bean>

    <bean id="commonbeans1" class="com.github.leleact.jtest.spring.ioc.bean.CommonBean1">
        <property name="id" value="ABC"/>
        <property name="num" value="123"/>
    </bean>
</beans>